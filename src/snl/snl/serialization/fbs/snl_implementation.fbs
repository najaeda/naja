namespace SNL.FBS;

struct DesignID {
  db_id:byte;
  library_id:ushort;
  design_id:uint;
}

table Instance {
  model:DesignID;
}

table DesignImplementation {
  instances:[Instance];
}

table LibraryImplementation {
  id:ushort = 0;
  design_implementations:[DesignImplementation];
}

table DBImplementation {
  id:byte = 1;
  library_implementations:[LibraryImplementation];
}

root_type DBImplementation;