library (FF_test) {
  cell (FF) {
    ff ("IQ") {
	    next_state      : "D";
	    clocked_on      : "CK";
	}
    pin(D) {
      direction : input ;
      timing () {
            related_pin	   : "CK";
			timing_type	   : hold_rising;
      }
      timing () {
			related_pin	   : "CK";
			timing_type	   : setup_rising;
      }
    }
    pin(CK) {
      direction : input ;  
    }
    pin(Q) {
      direction : output ;
      function		: "IQ";
      timing () {
        related_pin	   : "CK";
		timing_type	   : rising_edge;
		timing_sense   : non_unate;
      }
    }
  }
}